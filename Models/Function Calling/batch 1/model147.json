"{\n  \"activities\": [\n    {\n      \"name\": \"Identify Social Issues\",\n      \"description\": \"Identify social issues to address in the community.\"\n    },\n    {\n      \"name\": \"Develop CSR Strategy\",\n      \"description\": \"Develop a corporate social responsibility (CSR) strategy.\"\n    },\n    {\n      \"name\": \"Allocate Resources\",\n      \"description\": \"Allocate resources for CSR initiatives.\"\n    },\n    {\n      \"name\": \"Implement CSR Initiative\",\n      \"description\": \"Implement the CSR initiative in the targeted area.\"\n    },\n    {\n      \"name\": \"Monitor Progress\",\n      \"description\": \"Monitor the progress of CSR initiatives.\"\n    },\n    {\n      \"name\": \"Engage Employees\",\n      \"description\": \"Engage employees in CSR activities.\"\n    },\n    {\n      \"name\": \"Report Outcomes\",\n      \"description\": \"Report outcomes of CSR initiatives to stakeholders.\"\n    },\n    {\n      \"name\": \"Assess Impact\",\n      \"description\": \"Assess the impact of CSR initiatives.\"\n    },\n    {\n      \"name\": \"Revise Strategy\",\n      \"description\": \"Revise CSR strategy based on assessment.\"\n    },\n    {\n      \"name\": \"Celebrate CSR Successes\",\n      \"description\": \"Celebrate successes of CSR initiatives with employees and partners.\"\n    }\n  ],\n  \"attributes\": [\n    {\n      \"type\": \"integer\",\n      \"name\": \"CSR Budget\",\n      \"description\": \"Total budget allocated for CSR initiatives.\",\n      \"min_value\": 1000,\n      \"max_value\": 100000\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Initiative Type\",\n      \"description\": \"Type of CSR initiative.\",\n      \"enumeration_values\": [\"Environmental\", \"Educational\", \"Health\"]\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Employee Engagement Level\",\n      \"description\": \"Level of employee engagement in CSR activities.\",\n      \"enumeration_values\": [\"Low\", \"Medium\", \"High\"]\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Number of Volunteers\",\n      \"description\": \"Number of volunteers participating in CSR activities.\",\n      \"min_value\": 1,\n      \"max_value\": 500\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Progress Status\",\n      \"description\": \"Status of CSR initiative progress.\",\n      \"enumeration_values\": [\"Not Started\", \"In Progress\", \"Completed\"]\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Impact Score\",\n      \"description\": \"Impact score of CSR initiatives based on assessments.\",\n      \"min_value\": 0.0,\n      \"max_value\": 10.0\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Stakeholder Satisfaction\",\n      \"description\": \"Satisfaction level of stakeholders with CSR outcomes.\",\n      \"enumeration_values\": [\"Dissatisfied\", \"Neutral\", \"Satisfied\"]\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Identify Social Issues\",\n        \"description\": \"Identify social issues to address in the community.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Initiative Type\",\n          \"description\": \"Type of CSR initiative.\",\n          \"enumeration_values\": [\"Environmental\", \"Educational\", \"Health\"]\n        }\n      ],\n      \"description\": \"Bind Initiative Type to 'Identify Social Issues' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Allocate Resources\",\n        \"description\": \"Allocate resources for CSR initiatives.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"CSR Budget\",\n          \"description\": \"Total budget allocated for CSR initiatives.\",\n          \"min_value\": 1000,\n          \"max_value\": 100000\n        }\n      ],\n      \"description\": \"Bind CSR Budget to 'Allocate Resources' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Engage Employees\",\n        \"description\": \"Engage employees in CSR activities.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Employee Engagement Level\",\n          \"description\": \"Level of employee engagement in CSR activities.\",\n          \"enumeration_values\": [\"Low\", \"Medium\", \"High\"]\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Number of Volunteers\",\n          \"description\": \"Number of volunteers participating in CSR activities.\",\n          \"min_value\": 1,\n          \"max_value\": 500\n        }\n      ],\n      \"description\": \"Bind Employee Engagement Level and Number of Volunteers to 'Engage Employees' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Monitor Progress\",\n        \"description\": \"Monitor the progress of CSR initiatives.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Progress Status\",\n          \"description\": \"Status of CSR initiative progress.\",\n          \"enumeration_values\": [\"Not Started\", \"In Progress\", \"Completed\"]\n        }\n      ],\n      \"description\": \"Bind Progress Status to 'Monitor Progress' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Assess Impact\",\n        \"description\": \"Assess the impact of CSR initiatives.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"float\",\n          \"name\": \"Impact Score\",\n          \"description\": \"Impact score of CSR initiatives based on assessments.\",\n          \"min_value\": 0.0,\n          \"max_value\": 10.0\n        }\n      ],\n      \"description\": \"Bind Impact Score to 'Assess Impact' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Report Outcomes\",\n        \"description\": \"Report outcomes of CSR initiatives to stakeholders.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Stakeholder Satisfaction\",\n          \"description\": \"Satisfaction level of stakeholders with CSR outcomes.\",\n          \"enumeration_values\": [\"Dissatisfied\", \"Neutral\", \"Satisfied\"]\n        }\n      ],\n      \"description\": \"Bind Stakeholder Satisfaction to 'Report Outcomes' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"template\": \"Init\",\n      \"activation\": {\n        \"name\": \"Identify Social Issues\",\n        \"description\": \"Identify social issues to address in the community.\"\n      },\n      \"description\": \"Identify Social Issues must start the process.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Develop CSR Strategy\",\n        \"description\": \"Develop a corporate social responsibility (CSR) strategy.\"\n      },\n      \"target\": {\n        \"name\": \"Allocate Resources\",\n        \"description\": \"Allocate resources for CSR initiatives.\"\n      },\n      \"description\": \"Developing CSR Strategy must precede resource allocation.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Allocate Resources\",\n        \"description\": \"Allocate resources for CSR initiatives.\"\n      },\n      \"target\": {\n        \"name\": \"Implement CSR Initiative\",\n        \"description\": \"Implement the CSR initiative in the targeted area.\"\n      },\n      \"description\": \"Resource allocation must be followed by CSR initiative implementation.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Succession\",\n      \"activation\": {\n        \"name\": \"Implement CSR Initiative\",\n        \"description\": \"Implement the CSR initiative in the targeted area.\"\n      },\n      \"target\": {\n        \"name\": \"Monitor Progress\",\n        \"description\": \"Monitor the progress of CSR initiatives.\"\n      },\n      \"description\": \"CSR initiative implementation must be immediately followed by progress monitoring.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Response\",\n      \"activation\": {\n        \"name\": \"Monitor Progress\",\n        \"description\": \"Monitor the progress of CSR initiatives.\"\n      },\n      \"target\": {\n        \"name\": \"Report Outcomes\",\n        \"description\": \"Report outcomes of CSR initiatives to stakeholders.\"\n      },\n      \"description\": \"Monitoring progress must be immediately followed by outcome reporting.\",\n      \"time_condition\": \"0,48,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Alternate Response\",\n      \"activation\": {\n        \"name\": \"Report Outcomes\",\n        \"description\": \"Report outcomes of CSR initiatives to stakeholders.\"\n      },\n      \"target\": {\n        \"name\": \"Assess Impact\",\n        \"description\": \"Assess the impact of CSR initiatives.\"\n      },\n      \"description\": \"Reporting outcomes must be followed by impact assessment.\",\n      \"time_condition\": \"0,72,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Assess Impact\",\n        \"description\": \"Assess the impact of CSR initiatives.\"\n      },\n      \"target\": {\n        \"name\": \"Revise Strategy\",\n        \"description\": \"Revise CSR strategy based on assessment.\"\n      },\n      \"description\": \"Impact assessment must precede CSR strategy revision.\",\n      \"time_condition\": \"0,24,h\"\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Absence\",\n      \"activation\": {\n        \"name\": \"Engage Employees\",\n        \"description\": \"Engage employees in CSR activities.\"\n      },\n      \"description\": \"Employee engagement must not occur more than 5 times.\",\n      \"cardinality\": 5\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Revise Strategy\",\n        \"description\": \"Revise CSR strategy based on assessment.\"\n      },\n      \"target\": {\n        \"name\": \"Celebrate CSR Successes\",\n        \"description\": \"Celebrate successes of CSR initiatives with employees and partners.\"\n      },\n      \"description\": \"CSR strategy revision must eventually be followed by CSR success celebrations.\",\n      \"time_condition\": \"1,7,d\"\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"End\",\n      \"activation\": {\n        \"name\": \"Celebrate CSR Successes\",\n        \"description\": \"Celebrate successes of CSR initiatives with employees and partners.\"\n      },\n      \"description\": \"Celebrating CSR successes must end the process.\"\n    }\n  ]\n}"