"{\"activities\":[{\"name\":\"Outline Story\",\"description\":\"Create a detailed outline of the story.\"},{\"name\":\"Write Draft\",\"description\":\"Write the first draft of the story.\"},{\"name\":\"Review Draft\",\"description\":\"Review the draft to identify areas for improvement.\"},{\"name\":\"Edit Draft\",\"description\":\"Make necessary edits to the draft.\"},{\"name\":\"Critique Draft\",\"description\":\"Get feedback from peers or editors.\"},{\"name\":\"Revise Draft\",\"description\":\"Revise the draft based on feedback.\"},{\"name\":\"Proofread Draft\",\"description\":\"Check the draft for grammar and spelling errors.\"},{\"name\":\"Submit Manuscript\",\"description\":\"Submit the final manuscript to the publisher.\"},{\"name\":\"Publish Story\",\"description\":\"Publish the story.\"},{\"name\":\"Market Story\",\"description\":\"Promote the story to reach a wider audience.\"},{\"name\":\"Collect Feedback\",\"description\":\"Collect feedback from readers.\"},{\"name\":\"Analyze Feedback\",\"description\":\"Analyze the feedback collected from readers.\"},{\"name\":\"Plan Book Launch\",\"description\":\"Organize and plan the book launch event.\"},{\"name\":\"Conduct Book Launch\",\"description\":\"Host the book launch event.\"},{\"name\":\"Celebrate Success\",\"description\":\"Celebrate the successful publication and launch of the book.\"}],\"attributes\":[{\"type\":\"integer\",\"name\":\"Word Count\",\"description\":\"The total number of words in the draft.\",\"min_value\":1000,\"max_value\":100000},{\"type\":\"float\",\"name\":\"Writing Speed\",\"description\":\"The speed at which the draft is written, in words per hour.\",\"min_value\":10.0,\"max_value\":1000.0},{\"type\":\"enumeration\",\"name\":\"Genre\",\"description\":\"The genre of the story.\",\"enumeration_values\":[\"Fiction\",\"Non-Fiction\",\"Sci-Fi\",\"Fantasy\",\"Mystery\",\"Romance\"]},{\"type\":\"integer\",\"name\":\"Revision Count\",\"description\":\"The number of revisions done on the dra