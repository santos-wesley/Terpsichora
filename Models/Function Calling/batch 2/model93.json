_value\": 100000.0\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Plan Strategy\",\n        \"description\": \"Develop a strategy for the tactical operation.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        }\n      ],\n      \"description\": \"Bind Operation ID to 'Plan Strategy' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Deploy Teams\",\n        \"description\": \"Deploy tactical teams to their designated positions.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Team ID\",\n          \"description\": \"Unique identifier for the team.\",\n          \"min_value\": 1,\n          \"max_value\": 100\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Team Designation\",\n          \"description\": \"Designation of the tactical team.\",\n          \"enumeration_values\": [\"Alpha\", \"Bravo\", \"Charlie\", \"Delta\"]\n        }\n      ],\n      \"description\": \"Bind Operation ID, Team ID, and Team Designation to 'Deploy Teams' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Recon Area\",\n        \"description\": \"Conduct reconnaissance of the operation area.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Recon Method\",\n          \"description\": \"Method used for reconnaissance.\",\n          \"enumeration_values\": [\"Drone\", \"Satellite\", \"On-Ground\"]\n        }\n      ],\n      \"description\": \"Bind Operation ID and Recon Method to 'Recon Area' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Extract Intel\",\n        \"description\": \"Extract intelligence from the target or area.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Intel Value\",\n          \"description\": \"Estimated value of the extracted intelligence.\",\n          \"min_value\": 0.1,\n          \"max_value\": 100.0\n        }\n      ],\n      \"description\": \"Bind Operation ID and Intel Value to 'Extract Intel' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Engage Target\",\n        \"description\": \"Engage the primary target in the operation.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Engagement Outcome\",\n          \"description\": \"Outcome of engaging the target.\",\n          \"enumeration_values\": [\"Success\", \"Failure\", \"Inconclusive\"]\n        }\n      ],\n      \"description\": \"Bind Operation ID and Engagement Outcome to 'Engage Target' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Assess Casualties\",\n        \"description\": \"Assess and document any casualties.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Casualty Count\",\n          \"description\": \"Number of casualties reported.\",\n          \"min_value\": 0.0,\n          \"max_value\": 50.0\n        }\n      ],\n      \"description\": \"Bind Operation ID and Casualty Count to 'Assess Casualties' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Evacuate Area\",\n        \"description\": \"Evacuate the operation area.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Evacuation Status\",\n          \"description\": \"Status of the evacuation process.\",\n          \"enumeration_values\": [\"Completed\", \"Ongoing\", \"Not Started\"]\n        }\n      ],\n      \"description\": \"Bind Operation ID and Evacuation Status to 'Evacuate Area' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Review Operation\",\n        \"description\": \"Conduct a review of the operation.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Review Outcome\",\n          \"description\": \"Outcome of the operation review.\",\n          \"enumeration_values\": [\"Positive\", \"Negative\", \"Neutral\"]\n        }\n      ],\n      \"description\": \"Bind Operation ID and Review Outcome to 'Review Operation' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Implement Adjustments\",\n        \"description\": \"Implement any adjustments based on the review.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Operation ID\",\n          \"description\": \"Unique identifier for the operation.\",\n          \"min_value\": 1,\n          \"max_value\": 10000\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Resource Allocation\",\n          \"description\": \"Resources allocated for adjustments.\",\n          \"min_value\": 1000.0,\n          \"max_value\": 100000.0\n        }\n      ],\n      \"description\": \"Bind Operation ID and Resource Allocation to 'Implement Adjustments' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"description\": \"Planning the strategy must be the first activity in the operation.\",\n      \"template\": \"Init\",\n      \"activation\": {\n        \"name\": \"Plan Strategy\",\n        \"description\": \"Develop a strategy for the tactical operation.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"description\": \"Restoring order must be the last activity in the operation.\",\n      \"template\": \"End\",\n      \"activation\": {\n        \"name\": \"Restore Order\",\n        \"description\": \"Restore order and normalcy post-operation.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Teams must be deployed after securing the perimeter.\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Deploy Teams\",\n        \"description\": \"Deploy tactical teams to their designated positions.\"\n      },\n      \"target\": {\n        \"name\": \"Secure Perimeter\",\n        \"description\": \"Establish a secure perimeter around the operation area.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Recon Area must follow the deployment of teams.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Deploy Teams\",\n        \"description\": \"Deploy tactical teams to their designated positions.\"\n      },\n      \"target\": {\n        \"name\": \"Recon Area\",\n        \"description\": \"Conduct reconnaissance of the operation area.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Extracting intelligence must follow engaging the target.\",\n      \"template\": \"Chain Response\",\n      \"activation\": {\n        \"name\": \"Engage Target\",\n        \"description\": \"Engage the primary target in the operation.\"\n      },\n      \"target\": {\n        \"name\": \"Extract Intel\",\n        \"description\": \"Extract intelligence from the target or area.\"\n      },\n      \"time_condition\": \"0,2,h\"\n    },\n    {\n      \"type\": \"unary\",\n      \"description\": \"Intel extraction must occur exactly once per operation.\",\n      \"template\": \"Exactly\",\n      \"activation\": {\n        \"name\": \"Extract Intel\",\n        \"description\": \"Extract intelligence from the target or area.\"\n      },\n      \"cardinality\": 1\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Intel must be verified after extraction.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Extract Intel\",\n        \"description\": \"Extract intelligence from the target or area.\"\n      },\n      \"target\": {\n        \"name\": \"Verify Intel\",\n        \"description\": \"Verify the extracted intelligence.\"\n      },\n      \"time_condition\": \"0,4,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Status report must follow verification of intelligence.\",\n      \"template\": \"Alternate Response\",\n      \"activation\": {\n        \"name\": \"Verify Intel\",\n        \"description\": \"Verify the extracted intelligence.\"\n      },\n      \"target\": {\n        \"name\": \"Report Status\",\n        \"description\": \"Report the status of the operation to command.\"\n      },\n      \"time_condition\": \"0,2,h\"\n    },\n    {\n      \"type\": \"unary\",\n      \"description\": \"Casualty assessment must occur at least once in the operation.\",\n      \"template\": \"Existence\",\n      \"activation\": {\n        \"name\": \"Assess Casualties\",\n        \"description\": \"Assess and document any casualties.\"\n      },\n      \"cardinality\": 1\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Evacuation must occur after status report.\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Evacuate Area\",\n        \"description\": \"Evacuate the operation area.\"\n      },\n      \"target\": {\n        \"name\": \"Report Status\",\n        \"description\": \"Report the status of the operation to command.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Intel delivery must follow evacuation.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Evacuate Area\",\n        \"description\": \"Evacuate the operation area.\"\n      },\n      \"target\": {\n        \"name\": \"Deliver Intel\",\n        \"description\": \"Deliver the verified intelligence to command.\"\n      },\n      \"time_condition\": \"0,8,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Debriefing teams must follow the delivery of intel.\",\n      \"template\": \"Chain Response\",\n      \"activation\": {\n        \"name\": \"Deliver Intel\",\n        \"description\": \"Deliver the verified intelligence to command.\"\n      },\n      \"target\": {\n        \"name\": \"Debrief Teams\",\n        \"description\": \"Debrief the teams involved in the operation.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Review operation must