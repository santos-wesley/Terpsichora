"{\n  \"activities\": [\n    {\n      \"name\": \"Define Backlog\",\n      \"description\": \"Create and prioritize the list of backlog items.\"\n    },\n    {\n      \"name\": \"Sprint Planning\",\n      \"description\": \"Plan the tasks and objectives for the upcoming sprint.\"\n    },\n    {\n      \"name\": \"Daily Standup\",\n      \"description\": \"Conduct a short daily meeting to discuss progress and obstacles.\"\n    },\n    {\n      \"name\": \"Develop Features\",\n      \"description\": \"Implement and build the agreed features.\"\n    },\n    {\n      \"name\": \"Test Features\",\n      \"description\": \"Verify and validate the new features developed.\"\n    },\n    {\n      \"name\": \"Sprint Review\",\n      \"description\": \"Review the completed work with the team and stakeholders.\"\n    },\n    {\n      \"name\": \"Sprint Retrospective\",\n      \"description\": \"Reflect on the sprint and discuss what went well and what can be improved.\"\n    },\n    {\n      \"name\": \"Deploy Increment\",\n      \"description\": \"Release the new increment to production.\"\n    },\n    {\n      \"name\": \"Gather Feedback\",\n      \"description\": \"Collect feedback from users and stakeholders.\"\n    },\n    {\n      \"name\": \"Refine Backlog\",\n      \"description\": \"Update and prioritize the backlog based on feedback and learned experiences.\"\n    }\n  ],\n  \"attributes\": [\n    {\n      \"type\": \"integer\",\n      \"name\": \"Sprint Number\",\n      \"description\": \"The identifier for the sprint number.\",\n      \"min_value\": 1,\n      \"max_value\": 100\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Task Status\",\n      \"description\": \"The current status of tasks being worked on.\",\n      \"enumeration_values\": [\"Not Started\", \"In Progress\", \"Completed\"]\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Feedback Type\",\n      \"description\": \"Type of feedback collected from stakeholders.\",\n      \"enumeration_values\": [\"Positive\", \"Negative\", \"Neutral\"]\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Story Points\",\n      \"description\": \"Effort estimation in story points for backlog items.\",\n      \"min_value\": 1,\n      \"max_value\": 20\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Meeting Outcome\",\n      \"description\": \"Resulting action items from the sprint retrospective.\",\n      \"enumeration_values\": [\"Action Required\", \"No Action\", \"Deferred\"]\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Completion Rate\",\n      \"description\": \"Percentage of tasks completed in a sprint.\",\n      \"min_value\": 0.0,\n      \"max_value\": 100.0\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Feature Count\",\n      \"description\": \"Number of features developed during the sprint.\",\n      \"min_value\": 1,\n      \"max_value\": 50\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Define Backlog\",\n        \"description\": \"Create and prioritize the list of backlog items.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Story Points\",\n          \"description\": \"Effort estimation in story points for backlog items.\",\n          \"min_value\": 1,\n          \"max_value\": 20\n        }\n      ],\n      \"description\": \"Bind Story Points to 'Define Backlog' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Sprint Planning\",\n        \"description\": \"Plan the tasks and objectives for the upcoming sprint.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sprint Number\",\n          \"description\": \"The identifier for the sprint number.\",\n          \"min_value\": 1,\n          \"max_value\": 100\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Feature Count\",\n          \"description\": \"Number of features developed during the sprint.\",\n          \"min_value\": 1,\n          \"max_value\": 50\n        }\n      ],\n      \"description\": \"Bind Sprint Number and Feature Count to 'Sprint Planning' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Daily Standup\",\n        \"description\": \"Conduct a short daily meeting to discuss progress and obstacles.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Task Status\",\n          \"description\": \"The current status of tasks being worked on.\",\n          \"enumeration_values\": [\"Not Started\", \"In Progress\", \"Completed\"]\n        }\n      ],\n      \"description\": \"Bind Task Status to 'Daily Standup' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Gather Feedback\",\n        \"description\": \"Collect feedback from users and stakeholders.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Feedback Type\",\n          \"description\": \"Type of feedback collected from stakeholders.\",\n          \"enumeration_values\": [\"Positive\", \"Negative\", \"Neutral\"]\n        }\n      ],\n      \"description\": \"Bind Feedback Type to 'Gather Feedback' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Sprint Retrospective\",\n        \"description\": \"Reflect on the sprint and discuss what went well and what can be improved.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Meeting Outcome\",\n          \"description\": \"Resulting action items from the sprint retrospective.\",\n          \"enumeration_values\": [\"Action Required\", \"No Action\", \"Deferred\"]\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Completion Rate\",\n          \"description\": \"Percentage of tasks completed in a sprint.\",\n          \"min_value\": 0.0,\n          \"max_value\": 100.0\n        }\n      ],\n      \"description\": \"Bind Meeting Outcome and Completion Rate to 'Sprint Retrospective' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"description\": \"Sprint Planning must start the process.\",\n      \"template\": \"Init\",\n      \"activation\": {\n        \"name\": \"Sprint Planning\",\n        \"description\": \"Plan the tasks and objectives for the upcoming sprint.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Develop Features activity must follow Sprint Planning.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Sprint Planning\",\n        \"description\": \"Plan the tasks and objectives for the upcoming sprint.\"\n      },\n      \"target\": {\n        \"name\": \"Develop Features\",\n        \"description\": \"Implement and build the agreed features.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Test Features activity must immediately follow Develop Features.\",\n      \"template\": \"Chain Succession\",\n      \"activation\": {\n        \"name\": \"Develop Features\",\n        \"description\": \"Implement and build the agreed features.\"\n      },\n      \"target\": {\n        \"name\": \"Test Features\",\n        \"description\": \"Verify and validate the new features developed.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"description\": \"Daily Standup must occur exactly 10 times during the process.\",\n      \"template\": \"Exactly\",\n      \"activation\": {\n        \"name\": \"Daily Standup\",\n        \"description\": \"Conduct a short daily meeting to discuss progress and obstacles.\"\n      },\n      \"cardinality\": 10\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Gather Feedback must occur after Sprint Review within 2 days.\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Sprint Review\",\n        \"description\": \"Review the completed work with the team and stakeholders.\"\n      },\n      \"target\": {\n        \"name\": \"Gather Feedback\",\n        \"description\": \"Collect feedback from users and stakeholders.\"\n      },\n      \"time_condition\": \"0,2,d\"\n    },\n    {\n      \"type\": \"unary\",\n      \"description\": \"Reflect on the sprint activity should be the last activity.\",\n      \"template\": \"End\",\n      \"activation\": {\n        \"name\": \"Sprint Retrospective\",\n        \"description\": \"Reflect on the sprint and discuss what went well and what can be improved.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Sprint Retrospective should be preceded by Sprint Review.\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Sprint Retrospective\",\n        \"description\": \"Reflect on the sprint and discuss what went well and what can be improved.\"\n      },\n      \"target\": {\n        \"name\": \"Sprint Review\",\n        \"description\": \"Review the completed work with the team and stakeholders.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Deploy Increment should occur within 3 hours after Test Features.\",\n      \"template\": \"Chain Response\",\n      \"activation\": {\n        \"name\": \"Test Features\",\n        \"description\": \"Verify and validate the new features developed.\"\n      },\n      \"target\": {\n        \"name\": \"Deploy Increment\",\n        \"description\": \"Release the new increment to production.\"\n      },\n      \"time_condition\": \"0,3,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Refine Backlog should occur after Gathering Feedback.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Gather Feedback\",\n        \"description\": \"Collect feedback from users and stakeholders.\"\n      },\n      \"target\": {\n        \"name\": \"Refine Backlog\",\n        \"description\": \"Update and prioritize the backlog based on feedback and learned experiences.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"description\": \"Sprint Retrospective should occur only if Completion Rate is less than 75%.\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Sprint Retrospective\",\n        \"description\": \"Reflect on the sprint and discuss what went well and what can be improved.\"\n      },\n      \"target\": {\n        \"name\": \"Sprint Review\",\n        \"description\": \"Review the completed work with the team and stakeholders.\"\n      },\n      \"activation_condition\": \"A.Completion Rate < 75\"\n    }\n  ]\n}"