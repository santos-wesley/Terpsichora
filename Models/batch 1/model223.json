"{\n  \"activities\": [\n    {\n      \"name\": \"Collect Sample\",\n      \"description\": \"Gather biological samples from the field.\"\n    },\n    {\n      \"name\": \"Isolate DNA\",\n      \"description\": \"Extract and isolate DNA from the biological samples.\"\n    },\n    {\n      \"name\": \"Amplify DNA\",\n      \"description\": \"Use PCR to amplify DNA sequences.\"\n    },\n    {\n      \"name\": \"Sequence DNA\",\n      \"description\": \"Determine the sequence of nucleotides in DNA.\"\n    },\n    {\n      \"name\": \"Analyze Data\",\n      \"description\": \"Analyze the DNA sequencing data.\"\n    },\n    {\n      \"name\": \"Record Observations\",\n      \"description\": \"Record observations from the DNA analysis.\"\n    },\n    {\n      \"name\": \"Interpret Results\",\n      \"description\": \"Interpret the results of the analysis.\"\n    },\n    {\n      \"name\": \"Store Sample\",\n      \"description\": \"Store the remaining samples for future use.\"\n    },\n    {\n      \"name\": \"Dispose Sample\",\n      \"description\": \"Safely dispose of used biological samples.\"\n    }\n  ],\n  \"attributes\": [\n    {\n      \"type\": \"integer\",\n      \"name\": \"Sample ID\",\n      \"description\": \"Unique identifier for each biological sample.\",\n      \"min_value\": 1,\n      \"max_value\": 999999\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Sample Type\",\n      \"description\": \"Type of biological sample collected.\",\n      \"enumeration_values\": [\"Blood\", \"Tissue\", \"Saliva\"]\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"DNA Concentration\",\n      \"description\": \"Concentration of isolated DNA.\",\n      \"min_value\": 1,\n      \"max_value\": 1000\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"PCR Protocol\",\n      \"description\": \"Protocol used for PCR amplification.\",\n      \"enumeration_values\": [\"Standard\", \"High-Fidelity\"]\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Sequencing Quality Score\",\n      \"description\": \"Quality score of the DNA sequencing.\",\n      \"min_value\": 0.0,\n      \"max_value\": 100.0\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Analysis Software\",\n      \"description\": \"Software used for DNA data analysis.\",\n      \"enumeration_values\": [\"Software A\", \"Software B\", \"Software C\"]\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Observation ID\",\n      \"description\": \"Unique identifier for each observation.\",\n      \"min_value\": 1,\n      \"max_value\": 99999\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Sample Storage Location\",\n      \"description\": \"Storage location identifier for the samples.\",\n      \"min_value\": 1,\n      \"max_value\": 100\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Disposal Method\",\n      \"description\": \"Method used for disposing of biological samples.\",\n      \"enumeration_values\": [\"Incineration\", \"Chemical Treatment\"]\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Collect Sample\",\n        \"description\": \"Gather biological samples from the field.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Sample Type\",\n          \"description\": \"Type of biological sample collected.\",\n          \"enumeration_values\": [\"Blood\", \"Tissue\", \"Saliva\"]\n        }\n      ],\n      \"description\": \"Bind Sample ID and Sample Type to 'Collect Sample' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Isolate DNA\",\n        \"description\": \"Extract and isolate DNA from the biological samples.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"DNA Concentration\",\n          \"description\": \"Concentration of isolated DNA.\",\n          \"min_value\": 1,\n          \"max_value\": 1000\n        }\n      ],\n      \"description\": \"Bind Sample ID and DNA Concentration to 'Isolate DNA' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Amplify DNA\",\n        \"description\": \"Use PCR to amplify DNA sequences.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"PCR Protocol\",\n          \"description\": \"Protocol used for PCR amplification.\",\n          \"enumeration_values\": [\"Standard\", \"High-Fidelity\"]\n        }\n      ],\n      \"description\": \"Bind Sample ID and PCR Protocol to 'Amplify DNA' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Sequence DNA\",\n        \"description\": \"Determine the sequence of nucleotides in DNA.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Sequencing Quality Score\",\n          \"description\": \"Quality score of the DNA sequencing.\",\n          \"min_value\": 0.0,\n          \"max_value\": 100.0\n        }\n      ],\n      \"description\": \"Bind Sample ID and Sequencig Quality Score to 'Sequence DNA' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Analyze Data\",\n        \"description\": \"Analyze the DNA sequencing data.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Analysis Software\",\n          \"description\": \"Software used for DNA data analysis.\",\n          \"enumeration_values\": [\"Software A\", \"Software B\", \"Software C\"]\n        }\n      ],\n      \"description\": \"Bind Sample ID and Analysis Software to 'Analyze Data' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Record Observations\",\n        \"description\": \"Record observations from the DNA analysis.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Observation ID\",\n          \"description\": \"Unique identifier for each observation.\",\n          \"min_value\": 1,\n          \"max_value\": 99999\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        }\n      ],\n      \"description\": \"Bind Observation ID and Sample ID to 'Record Observations' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Store Sample\",\n        \"description\": \"Store the remaining samples for future use.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample Storage Location\",\n          \"description\": \"Storage location identifier for the samples.\",\n          \"min_value\": 1,\n          \"max_value\": 100\n        }\n      ],\n      \"description\": \"Bind Sample ID and Sample Storage Location to 'Store Sample' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Dispose Sample\",\n        \"description\": \"Safely dispose of used biological samples.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Sample ID\",\n          \"description\": \"Unique identifier for each biological sample.\",\n          \"min_value\": 1,\n          \"max_value\": 999999\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Disposal Method\",\n          \"description\": \"Method used for disposing of biological samples.\",\n          \"enumeration_values\": [\"Incineration\", \"Chemical Treatment\"]\n        }\n      ],\n      \"description\": \"Bind Sample ID and Disposal Method to 'Dispose Sample' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"template\": \"Init\",\n      \"activation\": {\n        \"name\": \"Collect Sample\",\n        \"description\": \"Gather biological samples from the field.\"\n      },\n      \"description\": \"Collect Sample must be the first activity to occur in the process.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Isolate DNA\",\n        \"description\": \"Extract and isolate DNA from the biological samples.\"\n      },\n      \"target\": {\n        \"name\": \"Collect Sample\",\n        \"description\": \"Gather biological samples from the field.\"\n      },\n      \"description\": \"Isolate DNA must be preceded by Collect Sample.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Alternate Response\",\n      \"activation\": {\n        \"name\": \"Amplify DNA\",\n        \"description\": \"Use PCR to amplify DNA sequences.\"\n      },\n      \"target\": {\n        \"name\": \"Sequence DNA\",\n        \"description\": \"Determine the sequence of nucleotides in DNA.\"\n      },\n      \"description\": \"Sequences of Amplify DNA must be followed by Sequence DNA with no intermediate Amplify DNA.\",\n      \"time_condition\": \"0,12,h\"\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Existence\",\n      \"activation\": {\n        \"name\": \"Record Observations\",\n        \"description\": \"Record observations from the DNA analysis.\"\n      },\n      \"description\": \"Record Observations must occur at least once.\",\n      \"cardinality\": 1\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Succession\",\n      \"activation\": {\n        \"name\": \"Sequence DNA\",\n        \"description\": \"Determine the sequence of nucleotides in DNA.\"\n      },\n      \"target\": {\n        \"name\": \"Analyze Data\",\n        \"description\": \"Analyze the DNA sequencing data.\"\n      },\n      \"description\": \"Sequence DNA must be immediately followed by Analyze Data.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Response\",\n      \"activation\": {\n        \"name\": \"Analyze Data\",\n        \"description\": \"Analyze the DNA sequencing data.\"\n      },\n      \"target\": {\n        \"name\": \"Record Observations\",\n        \"description\": \"Record observations from the DNA analysis.\"\n      },\n      \"description\": \"If Analyze Data occurs, then Record Observations must eventually follow.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Not Co-Existence\",\n      \"activation\": {\n        \"name\": \"Store Sample\",\n        \"description\": \"Store the remaining samples for future use.\"\n      },\n      \"target\": {\n        \"name\": \"Dispose Sample\",\n        \"description\": \"Safely dispose of used biological samples.\"\n      },\n      \"description\": \"Store Sample and Dispose Sample cannot both occur in the process.\"\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"End\",\n      \"activation\": {\n        \"name\": \"Dispose Sample\",\n        \"description\": \"Safely dispose of used biological samples.\"\n      },\n      \"description\": \"Dispose Sample must be the last activity to occur in the process.\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Response\",\n      \"activation\": {\n        \"name\": \"Amplify DNA\",\n        \"description\": \"Use PCR to amplify DNA sequences.\"\n      },\n      \"target\": {\n        \"name\": \"Sequence DNA\",\n        \"description\": \"Determine the sequence of nucleotides in DNA.\"\n      },\n      \"description\": \"Amplify DNA must be immediately followed by Sequence DNA.\",\n      \"time_condition\": \"0,6,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"activation\": {\n        \"name\": \"Dispose Sample\",\n        \"description\": \"Safely dispose of used biological samples.\"\n      },\n      \"target\": {\n        \"name\": \"Store Sample\",\n        \"description\": \"Store the remaining samples for future use.\"\n      },\n      \"description\": \"Dispose Sample must be preceded by Store Sample.\",\n      \"time_condition\": \"0,24,h\"\n    }\n  ]\n}"