"{\n  \"activities\": [\n    {\n      \"name\": \"Collect Feedback\",\n      \"description\": \"Gather feedback from customers regarding their experience.\"\n    },\n    {\n      \"name\": \"Analyze Feedback\",\n      \"description\": \"Evaluate and interpret the collected customer feedback.\"\n    },\n    {\n      \"name\": \"Identify Issues\",\n      \"description\": \"Determine common issues from the analyzed feedback.\"\n    },\n    {\n      \"name\": \"Plan Improvement\",\n      \"description\": \"Devise a plan to address the identified issues.\"\n    },\n    {\n      \"name\": \"Implement Changes\",\n      \"description\": \"Put the improvement plan into action.\"\n    },\n    {\n      \"name\": \"Monitor Results\",\n      \"description\": \"Track the outcomes of the implemented changes.\"\n    },\n    {\n      \"name\": \"Update Customers\",\n      \"description\": \"Inform customers about the improvements made based on their feedback.\"\n    },\n    {\n      \"name\": \"Follow Up\",\n      \"description\": \"Check back with customers to gather feedback on the recent changes.\"\n    },\n    {\n      \"name\": \"Document Outcomes\",\n      \"description\": \"Record the results and insights from the feedback process.\"\n    },\n    {\n      \"name\": \"Review Process\",\n      \"description\": \"Evaluate the overall feedback analysis process for future iterations.\"\n    }\n  ],\n  \"attributes\": [\n    {\n      \"type\": \"integer\",\n      \"name\": \"Feedback Score\",\n      \"description\": \"Numeric score given by the customer for their experience.\",\n      \"min_value\": 1,\n      \"max_value\": 10\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Issue Type\",\n      \"description\": \"Classification of identified issue.\",\n      \"enumeration_values\": [\"Service\", \"Product\", \"Delivery\", \"Other\"]\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Improvement Plan\",\n      \"description\": \"Description of the plan to address issues.\",\n      \"enumeration_values\": [\"Training\", \"Product Enhancement\", \"Process Change\", \"Other\"]\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Improvement Cost\",\n      \"description\": \"Cost of implementing the improvement.\",\n      \"min_value\": 0,\n      \"max_value\": 10000\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Duration\",\n      \"description\": \"Time taken to implement changes in days.\",\n      \"min_value\": 1,\n      \"max_value\": 365\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Gather feedback from customers regarding their experience.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Feedback Score\",\n          \"description\": \"Numeric score given by the customer for their experience.\",\n          \"min_value\": 1,\n          \"max_value\": 10\n        }\n      ],\n      \"description\": \"Bind Feedback Score to 'Collect Feedback' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Identify Issues\",\n        \"description\": \"Determine common issues from the analyzed feedback.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Issue Type\",\n          \"description\": \"Classification of identified issue.\",\n          \"enumeration_values\": [\"Service\", \"Product\", \"Delivery\", \"Other\"]\n        }\n      ],\n      \"description\": \"Bind Issue Type to 'Identify Issues' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Plan Improvement\",\n        \"description\": \"Devise a plan to address the identified issues.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Improvement Plan\",\n          \"description\": \"Description of the plan to address issues.\",\n          \"enumeration_values\": [\"Training\", \"Product Enhancement\", \"Process Change\", \"Other\"]\n        },\n        {\n          \"type\": \"float\",\n          \"name\": \"Improvement Cost\",\n          \"description\": \"Cost of implementing the improvement.\",\n          \"min_value\": 0,\n          \"max_value\": 10000\n        }\n      ],\n      \"description\": \"Bind Improvement Plan and Improvement Cost to 'Plan Improvement' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Implement Changes\",\n        \"description\": \"Put the improvement plan into action.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Improvement Plan\",\n          \"description\": \"Description of the plan to address issues.\",\n          \"enumeration_values\": [\"Training\", \"Product Enhancement\", \"Process Change\", \"Other\"]\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Duration\",\n          \"description\": \"Time taken to implement changes in days.\",\n          \"min_value\": 1,\n          \"max_value\": 365\n        }\n      ],\n      \"description\": \"Bind Improvement Plan and Duration to 'Implement Changes' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"template\": \"Init\",\n      \"description\": \"Collect feedback must start the process\",\n      \"activation\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Gather feedback from customers regarding their experience.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Existence\",\n      \"description\": \"Analyze feedback at least once to gather insights.\",\n      \"activation\": {\n        \"name\": \"Analyze Feedback\",\n        \"description\": \"Evaluate and interpret the collected customer feedback.\"\n      },\n      \"cardinality\": 1\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"description\": \"Issues identification must occur after feedback analysis.\",\n      \"activation\": {\n        \"name\": \"Identify Issues\",\n        \"description\": \"Determine common issues from the analyzed feedback.\"\n      },\n      \"target\": {\n        \"name\": \"Analyze Feedback\",\n        \"description\": \"Evaluate and interpret the collected customer feedback.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Response\",\n      \"description\": \"Improvements must follow an improvement plan.\",\n      \"activation\": {\n        \"name\": \"Plan Improvement\",\n        \"description\": \"Devise a plan to address the identified issues.\"\n      },\n      \"target\": {\n        \"name\": \"Implement Changes\",\n        \"description\": \"Put the improvement plan into action.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Succession\",\n      \"description\": \"Documenting outcomes must immediately follow monitoring results.\",\n      \"activation\": {\n        \"name\": \"Monitor Results\",\n        \"description\": \"Track the outcomes of the implemented changes.\"\n      },\n      \"target\": {\n        \"name\": \"Document Outcomes\",\n        \"description\": \"Record the results and insights from the feedback process.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Absence\",\n      \"description\": \"No more than three follow-ups with customers.\",\n      \"activation\": {\n        \"name\": \"Follow Up\",\n        \"description\": \"Check back with customers to gather feedback on the recent changes.\"\n      },\n      \"cardinality\": 3\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Alternate Response\",\n      \"description\": \"Each feedback collection must be followed by feedback analysis within 24 hours.\",\n      \"activation\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Gather feedback from customers regarding their experience.\"\n      },\n      \"target\": {\n        \"name\": \"Analyze Feedback\",\n        \"description\": \"Evaluate and interpret the collected customer feedback.\"\n      },\n      \"time_condition\": \"0,24,h\"\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Precedence\",\n      \"description\": \"Review process can only occur if it is directly preceded by document outcomes.\",\n      \"activation\": {\n        \"name\": \"Review Process\",\n        \"description\": \"Evaluate the overall feedback analysis process for future iterations.\"\n      },\n      \"target\": {\n        \"name\": \"Document Outcomes\",\n        \"description\": \"Record the results and insights from the feedback process.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Not Precedence\",\n      \"description\": \"Update customers should not be preceded by issue identification.\",\n      \"activation\": {\n        \"name\": \"Update Customers\",\n        \"description\": \"Inform customers about the improvements made based on their feedback.\"\n      },\n      \"target\": {\n        \"name\": \"Identify Issues\",\n        \"description\": \"Determine common issues from the analyzed feedback.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"description\": \"Monitoring results must occur within 7 days after implementing changes.\",\n      \"activation\": {\n        \"name\": \"Monitor Results\",\n        \"description\": \"Track the outcomes of the implemented changes.\"\n      },\n      \"target\": {\n        \"name\": \"Implement Changes\",\n        \"description\": \"Put the improvement plan into action.\"\n      },\n      \"time_condition\": \"0,7,d\"\n    }\n  ]\n}"