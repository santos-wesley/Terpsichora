"{\n  \"activities\": [\n    {\n      \"name\": \"Gather Data\",\n      \"description\": \"Collect raw data from diverse sources.\"\n    },\n    {\n      \"name\": \"Clean Data\",\n      \"description\": \"Remove noise and irrelevant information from the data.\"\n    },\n    {\n      \"name\": \"Transform Data\",\n      \"description\": \"Change the format or structure of the data to suit the analysis.\"\n    },\n    {\n      \"name\": \"Analyze Data\",\n      \"description\": \"Perform statistical and predictive analysis on the data.\"\n    },\n    {\n      \"name\": \"Visualize Data\",\n      \"description\": \"Create visual representations and dashboards for the data.\"\n    },\n    {\n      \"name\": \"Present Findings\",\n      \"description\": \"Present the insights and findings to stakeholders.\"\n    },\n    {\n      \"name\": \"Deploy Model\",\n      \"description\": \"Deploy the predictive model into production environment.\"\n    },\n    {\n      \"name\": \"Monitor Model\",\n      \"description\": \"Continuously monitor the performance of the deployed model.\"\n    },\n    {\n      \"name\": \"Collect Feedback\",\n      \"description\": \"Collect feedback on the model's performance from users.\"\n    },\n    {\n      \"name\": \"Retrain Model\",\n      \"description\": \"Retrain the model using new data to improve its accuracy.\"\n    }\n  ],\n  \"attributes\": [\n    {\n      \"type\": \"integer\",\n      \"name\": \"Data Size\",\n      \"description\": \"Size of the dataset in gigabytes.\",\n      \"min_value\": 1,\n      \"max_value\": 1000\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Data Quality\",\n      \"description\": \"Quality rating of the data.\",\n      \"enumeration_values\": [\"Low\", \"Medium\", \"High\"]\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Transformation Steps\",\n      \"description\": \"Number of transformation steps applied to the data.\",\n      \"min_value\": 1,\n      \"max_value\": 20\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Accuracy Rate\",\n      \"description\": \"Accuracy rate of the predictive model.\",\n      \"min_value\": 0.0,\n      \"max_value\": 1.0\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Model Type\",\n      \"description\": \"Type of predictive model used.\",\n      \"enumeration_values\": [\"Linear Regression\", \"Decision Tree\", \"Neural Network\"]\n    },\n    {\n      \"type\": \"enumeration\",\n      \"name\": \"Visualization Tool\",\n      \"description\": \"Tool used for visualizing data.\",\n      \"enumeration_values\": [\"Tableau\", \"Power BI\", \"Matplotlib\"]\n    },\n    {\n      \"type\": \"float\",\n      \"name\": \"Feedback Score\",\n      \"description\": \"Average score from user feedback.\",\n      \"min_value\": 0.0,\n      \"max_value\": 10.0\n    },\n    {\n      \"type\": \"integer\",\n      \"name\": \"Deployment Instances\",\n      \"description\": \"Number of instances where the model is deployed.\",\n      \"min_value\": 1,\n      \"max_value\": 100\n    }\n  ],\n  \"binds\": [\n    {\n      \"activity\": {\n        \"name\": \"Gather Data\",\n        \"description\": \"Collect raw data from diverse sources.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Data Size\",\n          \"description\": \"Size of the dataset in gigabytes.\",\n          \"min_value\": 1,\n          \"max_value\": 1000\n        },\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Data Quality\",\n          \"description\": \"Quality rating of the data.\",\n          \"enumeration_values\": [\"Low\", \"Medium\", \"High\"]\n        }\n      ],\n      \"description\": \"Bind Data Size and Data Quality to 'Gather Data' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Transform Data\",\n        \"description\": \"Change the format or structure of the data to suit the analysis.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"integer\",\n          \"name\": \"Transformation Steps\",\n          \"description\": \"Number of transformation steps applied to the data.\",\n          \"min_value\": 1,\n          \"max_value\": 20\n        }\n      ],\n      \"description\": \"Bind Transformation Steps to 'Transform Data' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Deploy Model\",\n        \"description\": \"Deploy the predictive model into production environment.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"enumeration\",\n          \"name\": \"Model Type\",\n          \"description\": \"Type of predictive model used.\",\n          \"enumeration_values\": [\"Linear Regression\", \"Decision Tree\", \"Neural Network\"]\n        },\n        {\n          \"type\": \"integer\",\n          \"name\": \"Deployment Instances\",\n          \"description\": \"Number of instances where the model is deployed.\",\n          \"min_value\": 1,\n          \"max_value\": 100\n        }\n      ],\n      \"description\": \"Bind Model Type and Deployment Instances to 'Deploy Model' activity.\"\n    },\n    {\n      \"activity\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Collect feedback on the model's performance from users.\"\n      },\n      \"attributes\": [\n        {\n          \"type\": \"float\",\n          \"name\": \"Feedback Score\",\n          \"description\": \"Average score from user feedback.\",\n          \"min_value\": 0.0,\n          \"max_value\": 10.0\n        }\n      ],\n      \"description\": \"Bind Feedback Score to 'Collect Feedback' activity.\"\n    }\n  ],\n  \"constraints\": [\n    {\n      \"type\": \"unary\",\n      \"template\": \"Init\",\n      \"description\": \"Data gathering must initiate the data science process.\",\n      \"activation\": {\n        \"name\": \"Gather Data\",\n        \"description\": \"Collect raw data from diverse sources.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Response\",\n      \"description\": \"Data cleaning must follow data gathering.\",\n      \"activation\": {\n        \"name\": \"Gather Data\",\n        \"description\": \"Collect raw data from diverse sources.\"\n      },\n      \"target\": {\n        \"name\": \"Clean Data\",\n        \"description\": \"Remove noise and irrelevant information from the data.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Succession\",\n      \"description\": \"Data transformation must directly follow data cleaning.\",\n      \"activation\": {\n        \"name\": \"Clean Data\",\n        \"description\": \"Remove noise and irrelevant information from the data.\"\n      },\n      \"target\": {\n        \"name\": \"Transform Data\",\n        \"description\": \"Change the format or structure of the data to suit the analysis.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Precedence\",\n      \"description\": \"Data analysis must be preceded by data transformation.\",\n      \"activation\": {\n        \"name\": \"Analyze Data\",\n        \"description\": \"Perform statistical and predictive analysis on the data.\"\n      },\n      \"target\": {\n        \"name\": \"Transform Data\",\n        \"description\": \"Change the format or structure of the data to suit the analysis.\"\n      }\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Succession\",\n      \"description\": \"Visualizing data must follow data analysis and must be followed by presenting findings.\",\n      \"activation\": {\n        \"name\": \"Analyze Data\",\n        \"description\": \"Perform statistical and predictive analysis on the data.\"\n      },\n      \"target\": {\n        \"name\": \"Visualize Data\",\n        \"description\": \"Create visual representations and dashboards for the data.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Existence\",\n      \"description\": \"At least one model must be deployed.\",\n      \"activation\": {\n        \"name\": \"Deploy Model\",\n        \"description\": \"Deploy the predictive model into production environment.\"\n      },\n      \"cardinality\": 1\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Chain Response\",\n      \"time_condition\": \"0,24,h\",\n      \"description\": \"Model monitoring must directly follow deployment within 24 hours.\",\n      \"activation\": {\n        \"name\": \"Deploy Model\",\n        \"description\": \"Deploy the predictive model into production environment.\"\n      },\n      \"target\": {\n        \"name\": \"Monitor Model\",\n        \"description\": \"Continuously monitor the performance of the deployed model.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"Absence\",\n      \"description\": \"No more than 3 feedback collections.\",\n      \"activation\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Collect feedback on the model's performance from users.\"\n      },\n      \"cardinality\": 3\n    },\n    {\n      \"type\": \"binary\",\n      \"template\": \"Alternate Response\",\n      \"description\": \"Retrain model must occasionally follow feedback collection.\",\n      \"activation\": {\n        \"name\": \"Collect Feedback\",\n        \"description\": \"Collect feedback on the model's performance from users.\"\n      },\n      \"target\": {\n        \"name\": \"Retrain Model\",\n        \"description\": \"Retrain the model using new data to improve its accuracy.\"\n      }\n    },\n    {\n      \"type\": \"unary\",\n      \"template\": \"End\",\n      \"description\": \"Retrain model must end the data science process.\",\n      \"activation\": {\n        \"name\": \"Retrain Model\",\n        \"description\": \"Retrain the model using new data to improve its accuracy.\"\n      }\n    }\n  ]\n}"